class SalaDeAula:
    def __init__(self, numero: int, capacidade: int,nome:int, disciplina:int):
        self.numero = numero
        self.capaciddade = capacidade
        self.nome = nome
        self.disciplina = disciplina

from classes_ex_03 import Pessoa
class Professor(Pessoa):

    def dar_aula(self, sala: SalaDeAula):
        print(f"O prof. {self.nome} de {self.disciplina} est√° dando aula na sala {sala.numero}.")
